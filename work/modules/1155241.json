  "source" : "{% set size = \"24px\" %}\n{% set borderRadius = \"3px\" %}\n\n{% set linkStyle = \"width:\"~size~\";border-width:0px;border:0px;text-decoration:none;\" %}\n{% set imgStyle = \"height:\"~size~\";width:\"~size~\";border-radius:\"~borderRadius~\";border-width:0px;border:0px;\" %}\n\n{% set base_link_url = page_meta.canonical_url %}\n{% set social_page_title = page_meta.html_title %}\n{% set logoBase = \"https://static.hubspot.com/final/img/common/icons/social/\" %}\n\n{% macro render_social_icon(social, network) %}\n  {% set enabled = social.enabled %}\n  {% set link = social.custom_link_format %}\n  \n  {% if enabled %}\n    {% set logo = logoBase ~ network ~ \"-24x24.png\" %}\n    {% set social_link_url = base_link_url %}\n    {% if \"?\" in base_link_url %}\n      {% set social_link_url = base_link_url ~ \"&\"|safe %}\n    {% else %}\n      {% set social_link_url = base_link_url ~ \"?\" %}\n    {% endif %}\n    {% set social_link_url = social_link_url ~ \"utm_medium=social&utm_source=\"|safe ~ network %}\n\n    {% set url = social_link_url %}\n    <a href=\"{{ link }}\" target=\"_blank\" style=\"{{ linkStyle }}\" >\n      <img src=\"{{ logo }}\" class=\"hs-image-widget hs-image-social-sharing-24\" style=\"{{ imgStyle }}\" width=\"{{ size }}\" hspace=\"0\" alt='{{ \"Share on \" ~ network }}' />\n    </a>\n  {% endif %}\n{% endmacro %}\n\n<div class=\"hs_cos_wrapper hs_cos_wrapper_widget hs_cos_wrapper_type_social_sharing\" data-hs-cos-general-type=\"widget\" data-hs-cos-type=\"social_sharing\">\n  {{ render_social_icon(module.facebook, 'facebook') }}\n  {{ render_social_icon(module.linkedin, 'linkedin') }}\n  {{ render_social_icon(module.twitter, 'twitter') }}\n  {{ render_social_icon(module.google_plus, 'googleplus') }}\n  {% if module.pinterest.pinterest_media %}\n    {% set pinterest_media = module.pinterest.pinterest_media %}\n    {{ render_social_icon(module.pinterest, 'pinterest') }}\n  {% endif %}\n  {{ render_social_icon(module.email, 'email') }}\n</div>",
